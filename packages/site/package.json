    "gatsby-plugin-sharp": "^5.12.3",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-sharp": "^5.12.3",
    "lucide-react": "^0.290.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-is": "^18.2.0",
