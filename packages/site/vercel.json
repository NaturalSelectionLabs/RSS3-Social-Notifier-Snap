{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000"
        },        
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-FRAME-OPTIONS",
          "value": "DENY"
        },
        {
          "key": " Access-Control-Allow-Origin",
          "value": "https://snap.rss3.io"
        }
      ]
    }
  ]
}
