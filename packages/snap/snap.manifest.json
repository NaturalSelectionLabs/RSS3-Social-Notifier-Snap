{
  "version": "0.1.0",
  "description": "RSS3 cron monitor your wallet address.",
  "proposedName": "RSS3 Cron",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmoosocool/rss3-snap.git"
  },
  "source": {
    "shasum": "6YvKpGu7VupbgBoFvnh+nSiglbcQ7sQMH4zShwNn2+A=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "rss3-cron",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "snap_notify": {},
    "snap_manageState": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "endowment:network-access": {},
    "endowment:ethereum-provider": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "execute"
          }
        }
      ]
    }
  },
  "manifestVersion": "0.1"
}
